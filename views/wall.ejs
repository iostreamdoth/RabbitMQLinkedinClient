	<script>
		
	</script>
	<script>
		var signinController = angular.module("signinapp", []);
		signinController.controller("signinController", function($scope, $http,
				$location) {

			$scope.signIn = function() {
				alert($scope.loginemail);
				$http({
					method : 'POST',
					url : '/signin',
					data : {
						"username" : $scope.loginemail,
						"password" : $scope.loginpassword
					}

				}).success(function(response) {
					if (response.status == "success")
						window.location = '/wall';
					else
						window.location = '/failLogin';
				}).error(function(error) {
					alert("error");
				});
			};
		});
		var wallcontroller = angular.module("wallcontrol", []);
		wallcontroller.controller("wallcontroller", function($scope, $http,
				$location) {

			$http({
				method : 'GET',
				url : '/wall/getwall',
			}).success(function(response) {
				console.log(response);

				$scope.feed = JSON.parse(response.feed);

			}).error(function(error) {
				alert("error");
			});

		});
		angular.bootstrap(document.getElementById("wallcontrol"),
				[ 'wallcontrol' ]);
	</script>
