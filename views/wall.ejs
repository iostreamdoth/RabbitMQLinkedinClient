			<div ng-repeat="entity in feed track by $index">
				<div class="container">
					<div class="row">
						<div class="col-md-2"></div>
						<div class="col-md-8 wallnotification margintop10px">
							<div ng-if="entity.imagedetail == '' ">
								<div class="container">
									<div class="row">
										<div class="col-md-1" style="border-right: 1px solid #000">
											<img height="75px" width="60px"
												src="/include/images/no-profile-img.gif"> <a>
												{{entity.firstname}} </a>
										</div>
										<div class="col-md-6">
											<h2>is now connected to</h2>
										</div>
										<div ng-if="entity.toimagedetail == '' " class="col-md-1">

											<img height="75px" width="60px"
												src="/include/images/no-profile-img.gif"> <a>
												{{entity.tofirstname}} </a>

										</div>
										<div ng-if="entity.toimagedetail != '' " class="col-md-1">

											<img height="75px" width="60px"
												src="/include/images/{{entity.toimagedetail}}"> <a>
												{{entity.tofirstname}} </a>

										</div>
									</div>
								</div>

							</div>
							<div ng-if="entity.imagedetail != '' ">
								<div class="container">
									<div class="row">
										<div class="col-md-1" style="border-right: 1px solid #000">
											<img height="75px" width="60px"
												src="/include/images/{{entity.imagedetail}}"> <a>
												{{entity.firstname}} </a>
										</div>
										<div class="col-md-6">
											<h2>is now connected to</h2>
										</div>
										<div ng-if="entity.toimagedetail == '' " class="col-md-1">

											<img height="75px" width="60px"
												src="/include/images/no-profile-img.gif"> <a>
												{{entity.tofirstname}} </a>

										</div>
										<div ng-if="entity.toimagedetail != '' " class="col-md-1">

											<img height="75px" width="60px"
												src="/include/images/{{entity.toimagedetail}}"> <a>
